{
	"schemaVersion": 1,
	"id": "ohse",
	"version": "${version}",
	"name": "Overhauled Horde Spawn Engine",
	"description": "OHSE is an advanced spawn engine that replaces the classic entity generation system with a horde-based one.\nDesigned to be ultra-configurable, efficient, and immersive, it allows you to manage the spawning of any mob in a natural and coherent way.",
	"authors": [
		"SirHephaistos"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/ohse",
		"sources": "https://www.curseforge.com/minecraft/mc-mods/ohse"
	},
	"license": "All Rights Reserved",
	"icon": "assets/ohse/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"as.sirhephaistos.ohse.OverhauledHordeSpawnEngine"
		],
		"client": [
			"as.sirhephaistos.ohse.OverhauledHordeSpawnEngineClient",
            "as.sirhephaistos.ohse.client.WandClient"
		],
		"fabric-datagen": [
			"as.sirhephaistos.ohse.OverhauledHordeSpawnEngineDataGenerator"
		]
	},
	"mixins": [
		"ohse.mixins.json",
		{
			"config": "ohse.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}